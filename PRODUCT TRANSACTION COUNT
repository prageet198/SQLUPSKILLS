URL:-https://platform.stratascratch.com/coding/10163-product-transaction-count?code_type=5

SELECT excel_sql_inventory_data.product_name,
COUNT (excel_sql_transaction_data.transaction_id) AS totalTransactions
FROM excel_sql_inventory_data
INNER JOIN excel_sql_transaction_data
ON excel_sql_inventory_data.product_id=excel_sql_transaction_data.product_id
GROUP BY excel_sql_inventory_data.product_id,excel_sql_inventory_data.product_name
ORDER BY excel_sql_inventory_data.product_id
